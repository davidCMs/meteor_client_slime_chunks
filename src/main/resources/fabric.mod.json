{
  "schemaVersion": 1,
  "id": "meteor_client_slime_chunks",
  "version": "${version}",
  "name": "Meteor Client Slime Chunks",
  "description": "Addon mod for meteor client that shows you slime chunks",
  "authors": [
    "davidCMs"
  ],
  "contact": {
    "issues": "https://github.com/davidCMs/meteor_client_slime_chunks/issues",
    "sources": "https://github.com/davidCMs/meteor_client_slime_chunks"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/meteor_client_slime_chunks/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "org.davidcms.meteor_client_slime_chunks.Meteor_client_slime_chunks"
    ]
  },
  "mixins": [
    "meteor_client_slime_chunks.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "25,225,25"
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "meteor-client": "*"
  }
}
